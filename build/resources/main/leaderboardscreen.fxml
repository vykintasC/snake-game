<?scenebuilder-stylesheet leaderboardscreen.css?>

<AnchorPane fx:id="anchorPaneMenuScreen" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LeaderBoardController">
   <children>
      <ImageView fitHeight="409.0" fitWidth="612.0" layoutY="2.0" opacity="0.5" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../snake_background_menu.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="409.0" fitWidth="612.0" layoutX="409.0" layoutY="2.0" opacity="0.5" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../snake_background_menu_2.png" />
         </image>
      </ImageView>
      <VBox alignment="BOTTOM_CENTER" layoutX="167.0" layoutY="40.0" prefHeight="321.0" prefWidth="267.0" styleClass="vbox">
         <children>
            <AnchorPane prefHeight="303.0" prefWidth="259.0">
               <children>
                  <Label fx:id="highscores" alignment="TOP_CENTER" layoutX="28.0" layoutY="7.0" prefHeight="184.0" prefWidth="207.0" styleClass="label_beatrecord" textAlignment="CENTER" />
                  <Button fx:id="goBack" alignment="BOTTOM_CENTER" layoutX="70.0" layoutY="223.0" mnemonicParsing="false" onAction="#goBackToPrev" prefHeight="30.0" prefWidth="120.0" styleClass="savescoreButton" text="Go back" />
