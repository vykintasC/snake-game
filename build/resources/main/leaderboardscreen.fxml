            </AnchorPane>
         </children>
         <padding>
            <Insets bottom="30.0" top="30.0" />
         </padding>
      </VBox>
   </children>
   <stylesheets>
      <URL value="@leaderboardscreen.css" />
   </stylesheets>
</AnchorPane>
